ctrl + alt + {j, k}
  bspc node -f 'last.floating.local' || bspc node -f {next,prev}.local

# Toggle fullscreen of window
super + f
	bspc node --state \~fullscreen

# Toggle monocle of desktop
super + w
  bspc desktop --layout next

# float toggle
super + i
	bspc query --nodes -n focused.tiled && state=floating || state=tiled; \
	bspc node --state \~$state

# move desktop
super + {Up,Down,Left,Right}
	bspc desktop -m {north,south,west,east} -f

# Focus/swap windows by direction
super + {_,shift + }{h,j,k,l}
	bspc node --{focus,swap} {west,south,north,east}

# Focus/move window to the biggest space
super + {_,shift + }m
	bspc node --{focus,swap} biggest

ctrl + alt + shift + {w,e,d,f,v,n,u,i,o,p,c}
	bspc node --to-desktop {w,e,d,f,v,n,u,i,o,p,c}

ctrl + alt + {w,e,d,f,v,n,u,i,o,p,c}
	bspc desktop --focus {w,e,d,f,v,n,u,i,o,p,c}

# Focus left/right occupied desktop, used to find missing node
ctrl + alt + {Left,Right}
	bspc desktop --focus {prev,next}.occupied

ctrl + F1
  pulseaudio-ctl mute
ctrl + F2
  pulseaudio-ctl down 3
ctrl + F3
  pulseaudio-ctl up 3
XF86AudioLowerVolume
  pulseaudio-ctl down 3
XF86AudioRaiseVolume
  pulseaudio-ctl up 3
XF86AudioMute
  pulseaudio-ctl mute
XF86MonBrightnessDown
  xbacklight -5
XF86MonBrightnessUp
  xbacklight +5
super + r
  bspc node --state pseudo_tiled; \
  sleep 0.1; \
  bspc node --state tiled

