#!/usr/bin/env bash

sudo pkill openconnect
sudo pkill core
netctl stop-all
sudo ip r d default
sudo ip r f table cn
sudo netctl switch-to "$1"
if [ $? -ne 0 ]; then
    systemctl stop zerotier-one
    systemctl stop dnsmasq
    sleep 5
    systemctl start zerotier-one
    systemctl start dnsmasq
fi

# addroutes
